{
  int read=0;
  while (read > -1) {
    cb.position(cb.position() + read);
    if (cb.remaining() == 0) {
      resizeCharBuffer();
    }
    read=r.read(cb.array(),cb.position(),cb.remaining());
  }
  cb.flip();
  try {
    invokeWebSocketProtocol(webSocket,cb.toString());
  }
  finally {
    cb.clear();
  }
}
