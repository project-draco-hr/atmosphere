{
  int read=0;
  while (read > -1) {
    bb.position(bb.position() + read);
    if (bb.remaining() == 0) {
      resizeByteBuffer();
    }
    read=is.read(bb.array(),bb.position(),bb.remaining());
  }
  bb.flip();
  try {
    invokeWebSocketProtocol(webSocket,bb.array(),0,bb.limit());
  }
  finally {
    bb.clear();
  }
}
