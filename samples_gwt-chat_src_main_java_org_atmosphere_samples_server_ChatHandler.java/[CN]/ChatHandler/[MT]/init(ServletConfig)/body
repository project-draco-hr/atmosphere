{
  super.init(servletConfig);
  Logger.getLogger("").setLevel(Level.INFO);
  Logger.getLogger("org.atmosphere.gwt").setLevel(Level.ALL);
  Logger.getLogger("org.atmosphere.samples").setLevel(Level.ALL);
