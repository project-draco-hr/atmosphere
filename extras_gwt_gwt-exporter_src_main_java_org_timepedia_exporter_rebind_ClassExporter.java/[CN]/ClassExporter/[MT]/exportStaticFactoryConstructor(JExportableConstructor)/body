{
  JExportableClassType consType=(JExportableClassType)constructor.getExportableReturnType();
  String typeName=consType.getQualifiedSourceName();
  sw.print("public static " + typeName + " "+ constructor.getStaticFactoryMethodName()+ "(");
  declareParameters(constructor,-1);
  sw.println(") {");
  sw.indent();
  sw.print("return new " + typeName + "(");
  declareJavaPassedValues(constructor,false);
  sw.println(");");
  sw.outdent();
  sw.println("}");
}
