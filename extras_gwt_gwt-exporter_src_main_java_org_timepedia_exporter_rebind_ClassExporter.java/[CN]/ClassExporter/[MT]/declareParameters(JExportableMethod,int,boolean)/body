{
  JExportableParameter params[]=method.getExportableParameters();
  int numParams=includeTypes || arity < 0 ? params.length : arity;
  for (int i=0; i < numParams; i++) {
    sw.print((includeTypes ? params[i].getTypeName() : "") + " " + ARG_PREFIX+ i);
    if (i < numParams - 1) {
      sw.print(", ");
    }
  }
}
