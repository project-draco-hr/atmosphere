{
  logger.trace("WebSocket.close()");
  try {
    if (r.request() != null && r.request().getAttribute(CLEAN_CLOSE) == null) {
      close();
    }
  }
 catch (  Exception ex) {
    logger.trace("",ex);
  }
  try {
    bb.clear();
    cb.clear();
    buffer.close(r);
  }
 catch (  Exception ex) {
    logger.trace("",ex);
  }
}
