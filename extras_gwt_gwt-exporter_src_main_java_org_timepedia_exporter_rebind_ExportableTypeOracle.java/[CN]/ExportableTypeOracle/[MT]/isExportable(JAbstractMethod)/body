{
  if (method instanceof JConstructor) {
    if (method.getParameters().length == 0 && method.isPublic()) {
      return true;
    }
  }
  return (isExportable(method.getEnclosingType()) && method.isPublic() && !isNotExportable(method.getAnnotation(NoExport.class))) || (isExportable(method.getAnnotation(Export.class)));
}
