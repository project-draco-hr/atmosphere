{
  WebSocketProcessor processor=null;
  String webSocketProcessorName=framework.getWebSocketProcessorClassName();
  if (!webSocketProcessorName.equalsIgnoreCase(DefaultWebSocketProcessor.class.getName())) {
    try {
      processor=(WebSocketProcessor)framework.newClassInstance(Thread.currentThread().getContextClassLoader().loadClass(webSocketProcessorName));
    }
 catch (    Exception ex) {
      try {
        processor=(WebSocketProcessor)framework.newClassInstance(getClass().getClassLoader().loadClass(webSocketProcessorName));
      }
 catch (      Exception ex2) {
      }
    }
  }
  if (processor == null) {
    processor=new DefaultWebSocketProcessor(framework);
  }
  return processor;
}
