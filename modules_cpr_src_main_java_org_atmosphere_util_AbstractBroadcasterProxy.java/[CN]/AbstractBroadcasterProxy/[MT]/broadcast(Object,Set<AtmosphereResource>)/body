{
  if (destroyed.get()) {
    logger.warn("This Broadcaster has been destroyed and cannot be used {}",getID());
    return null;
  }
  start();
  BroadcasterFuture<Object> f=new BroadcasterFuture<Object>(msg,this);
  try {
    outgoingBroadcast(msg);
  }
  finally {
    futureDone(f);
  }
  return f;
}
