{
  Broadcaster b1=BroadcasterFactory.getDefault().get("b1");
  Broadcaster b2=BroadcasterFactory.getDefault().get("b2");
  AtmosphereResource r=AtmosphereResourceFactory.getDefault().create(framework.getAtmosphereConfig(),b1,AtmosphereResponse.newInstance().request(AtmosphereRequest.newInstance()),mock(AsyncSupport.class),mock(AtmosphereHandler.class));
  assertNotNull(r);
  b2.addAtmosphereResource(r);
  assertNotNull(AtmosphereResourceFactory.getDefault().find(r.uuid()));
}
