{
  Broadcaster b1=BroadcasterFactory.getDefault().get("b1");
  Broadcaster b2=BroadcasterFactory.getDefault().get("b2");
  AtmosphereResource r=AtmosphereResourceFactory.getDefault().create(mock(AtmosphereConfig.class),b1,AtmosphereResponse.create().request(AtmosphereRequest.create()),mock(AsyncSupport.class),mock(AtmosphereHandler.class));
  assertNotNull(r);
  b2.addAtmosphereResource(r);
  assertNotNull(AtmosphereResourceFactory.getDefault().find(r.uuid()));
}
