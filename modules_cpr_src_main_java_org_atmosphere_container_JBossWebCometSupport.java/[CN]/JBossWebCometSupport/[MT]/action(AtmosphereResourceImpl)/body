{
  super.action(actionEvent);
  if (actionEvent.action().type == Action.TYPE.RESUME && actionEvent.isInScope()) {
    try {
      HttpEvent event=(HttpEvent)actionEvent.getRequest().getAttribute(HTTP_EVENT);
      resumed.offer(event);
      if (config.getInitParameter(ApplicationConfig.RESUME_AND_KEEPALIVE) == null || config.getInitParameter(ApplicationConfig.RESUME_AND_KEEPALIVE).equalsIgnoreCase("false")) {
        event.close();
      }
    }
 catch (    IOException ex) {
      logger.debug("",ex);
    }
  }
}
