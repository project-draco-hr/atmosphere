{
  instanceCount=0;
  AtmosphereRequest request=new AtmosphereRequestImpl.Builder().pathInfo("/inject/b123").method("GET").build();
  framework.doCometSupport(request,AtmosphereResponseImpl.newInstance());
  assertEquals(instanceCount,1);
  assertNotNull(r.get());
