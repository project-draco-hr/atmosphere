{
  String prefix="";
  boolean firstClientPackage=true;
  for (  String pkg : type.getPackage().getName().split("\\.")) {
    if (firstClientPackage && pkg.equals("client")) {
      firstClientPackage=false;
      continue;
    }
    prefix+=pkg;
    prefix+='.';
  }
  return prefix.substring(0,prefix.length() - 1);
}
