{
  if (destroyed.get()) {
    return futureDone(msg);
  }
  subset.retainAll(resources);
  start();
  Object newMsg=filter(msg);
  if (newMsg == null) {
    return futureDone(msg);
  }
  BroadcasterFuture<Object> f=new BroadcasterFuture<Object>(newMsg,subset.size());
  dispatchMessages(new Entry(newMsg,subset,f,msg));
  return f;
}
