{
  boolean b=forceAsyncIOWriter;
  try {
    validAsyncIOWriter();
    writeStatusAndHeaders();
    forceAsyncIOWriter=false;
    asyncIOWriter.write(AtmosphereResponse.this,bytes,start,offset);
  }
 catch (  IOException e) {
    handleException(e);
    throw e;
  }
 finally {
    forceAsyncIOWriter=b;
  }
}
