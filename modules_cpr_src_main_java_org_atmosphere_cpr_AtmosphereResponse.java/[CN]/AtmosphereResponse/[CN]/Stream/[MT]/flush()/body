{
  if (!validFlushOrClose())   return;
  writeStatusAndHeaders();
  boolean b=forceAsyncIOWriter;
  forceAsyncIOWriter=false;
  try {
    asyncIOWriter.flush(AtmosphereResponse.this);
  }
 catch (  IOException e) {
    handleException(e);
    throw e;
  }
 finally {
    forceAsyncIOWriter=b;
  }
}
