{
  boolean b=forceAsyncIOWriter;
  try {
    validAsyncIOWriter();
    writeStatusAndHeaders();
    forceAsyncIOWriter=false;
    asyncIOWriter.write(AtmosphereResponse.this,new String(chars));
  }
 catch (  IOException e) {
    handleException(e);
    throw new RuntimeException(e);
  }
 finally {
    forceAsyncIOWriter=b;
  }
}
