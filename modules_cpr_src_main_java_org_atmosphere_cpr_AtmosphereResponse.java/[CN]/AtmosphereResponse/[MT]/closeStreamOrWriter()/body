{
  try {
    boolean isUsingStream=(Boolean)request().getAttribute(PROPERTY_USE_STREAM);
    if (isUsingStream) {
      try {
        response.getOutputStream().close();
      }
 catch (      java.lang.IllegalStateException ex) {
      }
    }
 else {
      response.getWriter().close();
    }
  }
 catch (  IOException e) {
    logger.trace("",e);
  }
}
