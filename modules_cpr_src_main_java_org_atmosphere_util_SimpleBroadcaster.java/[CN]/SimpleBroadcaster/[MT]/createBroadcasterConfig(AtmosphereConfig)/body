{
  BroadcasterConfig bc=(BroadcasterConfig)config.properties().get(BroadcasterConfig.class.getName());
  if (bc == null) {
    bc=new BroadcasterConfig(config.framework().broadcasterFilters(),config,false,getID()).setScheduledExecutorService(ExecutorsFactory.getScheduler(config));
  }
  return bc;
}
