{
  logger.debug("WebSocket.onOpen.");
  try {
    webSocketProcessor=(WebSocketProcessor)JettyWebSocketHandler.class.getClassLoader().loadClass(webSocketProcessorClassName).getDeclaredConstructor(new Class[]{AtmosphereServlet.class,WebSocketSupport.class}).newInstance(new Object[]{atmosphereServlet,new Jetty8WebSocketSupport(connection)});
    webSocketProcessor.connect(request);
  }
 catch (  Exception e) {
    logger.warn("failed to connect to web socket",e);
  }
}
