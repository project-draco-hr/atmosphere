{
  if (messages != null) {
    for (    SocketIOPacketImpl msg : messages) {
switch (msg.getFrameType()) {
case MESSAGE:
case JSON:
case EVENT:
case ACK:
case ERROR:
        msg.setPadding(messages.size() > 1);
      sendMessage(msg.toString());
    break;
default :
  logger.error("Unknown SocketIOEvent msg = " + msg);
}
}
}
}
