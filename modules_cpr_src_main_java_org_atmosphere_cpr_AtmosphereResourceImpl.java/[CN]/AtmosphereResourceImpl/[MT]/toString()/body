{
  try {
    return "AtmosphereResource{" + "\n\t uuid=" + uuid + ",\n\t transport="+ transport()+ ",\n\t isInScope="+ isInScope+ ",\n\t isResumed="+ isResumed()+ ",\n\t isCancelled="+ isCancelled()+ ",\n\t isSuspended="+ isSuspended()+ ",\n\t broadcasters="+ broadcasters+ ",\n\t isClosedByClient="+ (event != null ? event.isClosedByClient() : false)+ ",\n\t isClosedByApplication="+ (event != null ? event.isClosedByApplication() : false)+ ",\n\t action="+ action+ '}';
  }
 catch (  NullPointerException ex) {
    return "AtmosphereResourceImpl{" + uuid + "}";
  }
}
