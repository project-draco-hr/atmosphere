{
  if (listeners.size() > 0) {
    logger.debug("Invoking listener with {}",event);
  }
 else {
    return;
  }
  if (event.isResuming() || event.isResumedOnTimeout()) {
    onResume(event);
  }
 else   if (event.isCancelled()) {
    onDisconnect(event);
  }
 else   if (!isSuspendEvent.getAndSet(true) && event.isSuspended()) {
    onSuspend(event);
  }
 else   if (event.throwable() != null) {
    onThrowable(event);
  }
 else {
    onBroadcast(event);
  }
}
