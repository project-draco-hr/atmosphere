{
  try {
    Broadcaster b=getBroadcaster();
    if (!isCancelled.get()) {
      removeFromAllBroadcasters();
    }
    broadcasters.clear();
    broadcasters.add(b);
    unregister();
    removeEventListeners();
  }
 catch (  Throwable t) {
    logger.trace("destroyResource",t);
  }
 finally {
    unregister();
    webSocket=null;
  }
}
