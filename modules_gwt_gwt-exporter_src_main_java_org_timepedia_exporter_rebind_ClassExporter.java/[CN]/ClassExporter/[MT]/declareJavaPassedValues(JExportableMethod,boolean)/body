{
  JExportableParameter params[]=method.getExportableParameters();
  for (int i=0; i < params.length; i++) {
    JExportableType eType=params[i].getExportableType();
    boolean needExport=eType != null && eType.needsExport();
    boolean isArray=eType instanceof JExportableArrayType;
    String arrayType=needExport && isArray ? ("L" + ((JExportableArrayType)eType).getJSNIReference()) : "";
    if (wrap && needExport) {
      sw.print("@org.timepedia.exporter.client.ExporterUtil::wrap(" + (isArray ? arrayType : "Lorg/timepedia/exporter/client/Exportable;") + ")(");
    }
    sw.print(ARG_PREFIX + i);
    if (wrap && needExport) {
      sw.print(")");
    }
    if (i < params.length - 1) {
      sw.print(", ");
    }
  }
}
