{
  addMethodHandler("encodeURL",voidMethodHandler).addMethodHandler("encodeRedirectURL",voidMethodHandler).addMethodHandler("getCharacterEncoding",new UTF8Handler()).addMethodHandler("getMajorVersion",new MethodHandler(){
    @Override public Object handle(    Object clazz,    Method method,    Object[] methodObjects){
      return new Integer(3);
    }
  }
);
}
