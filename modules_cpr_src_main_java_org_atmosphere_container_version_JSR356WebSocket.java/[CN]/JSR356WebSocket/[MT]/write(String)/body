{
  try {
    try {
      semaphore.acquireUninterruptibly();
      session.getAsyncRemote().sendText(s,new WriteResult(resource(),s));
    }
 catch (    IllegalStateException e) {
      semaphore.release();
      throw e;
    }
  }
 catch (  NullPointerException e) {
    patchGlassFish(e);
  }
  return this;
}
