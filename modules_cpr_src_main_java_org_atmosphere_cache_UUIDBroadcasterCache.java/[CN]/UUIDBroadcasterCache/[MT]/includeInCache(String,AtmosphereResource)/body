{
  boolean b=false;
  List<String> list=bannedResources.get(broadcasterId);
  if (list != null) {
    logger.debug("Resource {} is no longer excluded from Broadcasters ",r.uuid(),list);
    b=list.remove(r.uuid());
    if (list.isEmpty()) {
      bannedResources.remove(broadcasterId);
    }
  }
  return b;
}
