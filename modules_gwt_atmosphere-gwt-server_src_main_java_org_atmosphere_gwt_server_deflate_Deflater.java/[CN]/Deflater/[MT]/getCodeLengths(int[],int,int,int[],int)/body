{
  if ((idx & 0x200) != 0) {
    if (depth > maxCodeLen) {
      return 1;
    }
 else {
      blCount[depth]++;
      return 0;
    }
  }
  int s;
  if (depth == maxCodeLen) {
    blCount[maxCodeLen]++;
    s=-1;
  }
 else {
    s=0;
  }
  int n=tree[idx];
  int l=n & 0x3FF;
  int r=(n >>> 10) & 0x3FF;
  s+=getCodeLengths(tree,l,depth + 1,blCount,maxCodeLen);
  s+=getCodeLengths(tree,r,depth + 1,blCount,maxCodeLen);
  return s;
}
