{
  this.atmosphereHandler=atmosphereHandler;
  try {
    if (BroadcasterFactory.getDefault() != null) {
      this.broadcaster=BroadcasterFactory.getDefault().lookup(mapping,true);
    }
 else {
      this.mapping=mapping;
    }
  }
 catch (  Exception t) {
    throw new RuntimeException(t);
  }
}
