{
  boolean found=false;
  for (  AtmosphereInterceptor interceptor : interceptors) {
    if (interceptor.getClass().equals(c.getClass())) {
      found=true;
      break;
    }
  }
  if (!found) {
    c.configure(config);
    interceptors.addLast(c);
    logger.info("Installed AtmosphereInterceptor {}. ",c);
  }
  return this;
}
