{
  String s=sc.getInitParameter(ApplicationConfig.ATMOSPHERE_INTERCEPTORS);
  if (s != null) {
    String[] list=s.split(",");
    for (    String a : list) {
      try {
        AtmosphereInterceptor ai=(AtmosphereInterceptor)Thread.currentThread().getContextClassLoader().loadClass(a.trim()).newInstance();
        ai.configure(config);
        interceptors.add(ai);
      }
 catch (      InstantiationException e) {
        logger.warn("",e);
      }
catch (      IllegalAccessException e) {
        logger.warn("",e);
      }
catch (      ClassNotFoundException e) {
        logger.warn("",e);
      }
    }
  }
  interceptors.addLast(new SSEAtmosphereInterceptor());
  interceptors.addLast(new JSONPAtmosphereInterceptor());
  logger.info("Installed AtmosphereInterceptor {}",interceptors);
}
