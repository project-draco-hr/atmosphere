{
  if (!allowAllClassesScan) {
    annotationPackages.add(Processor.class.getPackage().getName());
  }
  String s=config.getInitParameter(ApplicationConfig.CUSTOM_ANNOTATION_PACKAGE);
  if (s != null) {
    String[] l=s.split(",");
    for (    String p : l) {
      annotationPackages.addLast(p);
    }
  }
}
