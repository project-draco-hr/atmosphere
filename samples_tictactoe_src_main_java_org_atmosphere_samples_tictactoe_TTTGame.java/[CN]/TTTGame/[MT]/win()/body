{
  if (winner != -1) {
    return winner;
  }
  for (int i=0; i < 8; i++) {
    int winSum=board[wins[i][0]] + board[wins[i][1]] + board[wins[i][2]];
    if (winSum == 3) {
      winner=2;
    }
 else     if (winSum == 30) {
      winner=1;
    }
  }
  if (winner == -1 && turnNum > 8) {
    winner=0;
  }
  return winner;
}
