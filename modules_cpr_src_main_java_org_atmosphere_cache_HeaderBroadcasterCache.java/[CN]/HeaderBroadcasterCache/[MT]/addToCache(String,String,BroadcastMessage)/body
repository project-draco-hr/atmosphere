{
  long now=System.nanoTime();
  CacheMessage cacheMessage=put(e,now,uuid);
  if (!uuid.equals(NULL)) {
    AtmosphereResource r=AtmosphereResourceFactory.getDefault().find(uuid);
    if (r != null) {
      r.getResponse().setHeader(X_CACHE_DATE,String.valueOf(now));
    }
  }
  return cacheMessage;
}
