{
  localAttributes.clear();
  if (bis != null) {
    try {
      bis.close();
    }
 catch (    IOException e) {
    }
  }
  if (br != null) {
    try {
      br.close();
    }
 catch (    IOException e) {
    }
  }
  headers.clear();
  queryStrings.clear();
  if (request != null) {
    request.removeAttribute(FrameworkConfig.ATMOSPHERE_RESOURCE);
    request=null;
  }
}
