  return new StringBuilder().append("\nName: ").append(name).append("\n").append("\n\tScope: ").append(scope).append("\n").append("\n\tBroasdcasterCache ").append(broadcasterCache).append("\n").append("\n\tAtmosphereResource: ").append(resources.size()).append("\n").append(this.getClass().getName()).append("@").append(this.hashCode()).toString();
