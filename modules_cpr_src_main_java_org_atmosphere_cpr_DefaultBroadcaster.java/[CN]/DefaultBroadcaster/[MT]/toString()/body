{
  return new StringBuilder(this.getClass().getName()).append("@").append(this.hashCode()).append("\n").append("\n\tName: ").append(name).append("\n").append("\n\tScope: ").append(scope).append("\n").append("\n\tBroasdcasterCache ").append(broadcasterCache).append("\n").append("\n\tAtmosphereResource: ").append(resources.size()).append("\n").toString();
