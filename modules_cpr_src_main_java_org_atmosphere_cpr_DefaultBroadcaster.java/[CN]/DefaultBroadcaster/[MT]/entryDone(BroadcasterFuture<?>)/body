{
  if (f != null) {
    if (bc.getExecutorService() != null) {
      bc.getExecutorService().submit(new Runnable(){
        @Override public void run(){
          notifyBroadcastListener();
          f.done();
        }
      }
);
    }
 else {
      notifyBroadcastListener();
      f.done();
    }
  }
}
