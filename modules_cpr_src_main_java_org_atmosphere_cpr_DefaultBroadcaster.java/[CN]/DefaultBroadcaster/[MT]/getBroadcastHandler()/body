{
  return new Runnable(){
    public void run(){
      Entry msg=null;
      try {
        msg=messages.take();
        bc.getExecutorService().submit(this);
        push(msg);
      }
 catch (      Throwable ex) {
        LoggerUtils.getLogger().log(Level.SEVERE,null,ex);
      }
 finally {
        if (msg != null) {
          if (msg.future instanceof BroadcasterFuture) {
            ((BroadcasterFuture)msg.future).done();
          }
 else {
            msg.future.cancel(true);
          }
        }
      }
    }
  }
;
}
