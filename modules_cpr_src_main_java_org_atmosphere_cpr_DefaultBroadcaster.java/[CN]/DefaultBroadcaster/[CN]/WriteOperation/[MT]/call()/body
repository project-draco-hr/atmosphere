{
  if (!completed.getAndSet(true)) {
    try {
      r.getAtmosphereHandler().onStateChange(e);
    }
 catch (    Throwable t) {
      onException(t,r);
    }
  }
 else {
    onException(new IOException("Unable to write after " + writeTimeoutInSecond),r);
    AtmosphereResourceImpl.class.cast(r).cancel();
  }
  return null;
}
