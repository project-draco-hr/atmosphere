{
  Object lockingObject=getLockingObject();
  super.destroy();
synchronized (lockingObject) {
    try {
      disconnectPublisher();
      disconnectSubscriber();
      if (jedisPool != null) {
        jedisPool.destroy();
      }
    }
 catch (    Throwable t) {
      logger.warn("Jedis error on close",t);
    }
 finally {
      config.properties().put(REDIS_SHARED_POOL,null);
    }
  }
}
