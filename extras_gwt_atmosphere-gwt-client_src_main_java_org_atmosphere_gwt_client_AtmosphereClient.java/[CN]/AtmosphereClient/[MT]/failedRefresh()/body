{
  refreshState=null;
  GWT.log("CometClient: Failed refesh");
  if (refreshQueue != null) {
    for (    Object object : refreshQueue) {
      if (object == REFRESH || object == DISCONNECT) {
      }
 else {
        doOnMessage((List<?>)object,primaryTransport);
      }
    }
    refreshQueue.clear();
  }
  doDisconnect();
  scheduleConnect(primaryTransport);
}
