{
  if (rpcRequest == null) {
    throw new NullPointerException("rpcRequest");
  }
  if (rpcRequest.getSerializationPolicy() == null) {
    throw new NullPointerException("serializationPolicy");
  }
  String responsePayload;
  responsePayload=RPC.encodeResponseForSuccess(rpcRequest.getMethod(),message,rpcRequest.getSerializationPolicy(),rpcRequest.getFlags());
  return responsePayload;
}
