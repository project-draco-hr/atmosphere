{
  final AtmosphereResource r=(AtmosphereResource)config.properties().get(Thread.currentThread().getName() + AtmosphereResource.class.getSimpleName());
  if (r == null) {
    return null;
  }
  final AtmosphereResponse response=r.getResponse();
  return (AtmosphereResponse)Proxy.newProxyInstance(this.getClass().getClassLoader(),new Class[]{AtmosphereResponse.class},new ThreadLocalInvoker(){
{
      set(response);
    }
    @Override public Object invoke(    Object proxy,    Method method,    Object[] args) throws Throwable {
      return method.invoke(response,args);
    }
  }
);
}
