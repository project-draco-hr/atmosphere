{
  aliveRequests.remove(r.getRequest());
  if (r.isInScope() && r.action().type == Action.TYPE.RESUME && (config.getInitParameter(AtmosphereServlet.RESUME_AND_KEEPALIVE) == null || config.getInitParameter(AtmosphereServlet.RESUME_AND_KEEPALIVE).equalsIgnoreCase("false"))) {
  }
 else {
    try {
      r.getResponse().flushBuffer();
    }
 catch (    IOException e) {
      logger.trace(e.getMessage(),e);
    }
  }
}
