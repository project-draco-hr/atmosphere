{
  try {
    if (AtmosphereFramework.broadcasterCacheClassName != null) {
      BroadcasterCache cache;
      try {
        cache=(BroadcasterCache)Thread.currentThread().getContextClassLoader().loadClass(AtmosphereFramework.broadcasterCacheClassName).newInstance();
      }
 catch (      ClassNotFoundException ex) {
        cache=(BroadcasterCache)getClass().getClassLoader().loadClass(AtmosphereFramework.broadcasterCacheClassName).newInstance();
      }
      InjectorProvider.getInjector().inject(cache);
      setBroadcasterCache(cache);
    }
  }
 catch (  InstantiationException e) {
    throw new RuntimeException(e);
  }
catch (  IllegalAccessException e) {
    throw new RuntimeException(e);
  }
catch (  ClassNotFoundException e) {
    throw new RuntimeException(e);
  }
}
