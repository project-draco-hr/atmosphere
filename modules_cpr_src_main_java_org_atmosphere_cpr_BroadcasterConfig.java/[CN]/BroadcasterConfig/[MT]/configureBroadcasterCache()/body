{
  try {
    String className=config.framework().getBroadcasterCacheClassName();
    if (className != null) {
      try {
        broadcasterCache=(BroadcasterCache)config.framework().newClassInstance(Thread.currentThread().getContextClassLoader().loadClass(className));
      }
 catch (      ClassNotFoundException ex) {
        broadcasterCache=(BroadcasterCache)config.framework().newClassInstance(getClass().getClassLoader().loadClass(className));
      }
      configureSharedCacheExecutor();
      broadcasterCache.configure(this);
    }
    for (    BroadcasterCacheInspector b : config.framework().inspectors()) {
      broadcasterCache.inspector(b);
    }
  }
 catch (  InstantiationException e) {
    throw new RuntimeException(e);
  }
catch (  IllegalAccessException e) {
    throw new RuntimeException(e);
  }
catch (  ClassNotFoundException e) {
    throw new RuntimeException(e);
  }
}
