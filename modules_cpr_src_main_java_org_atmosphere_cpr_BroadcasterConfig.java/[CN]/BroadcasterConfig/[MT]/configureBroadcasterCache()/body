{
  try {
    String className=config.framework().getBroadcasterCacheClassName();
    if (className != null) {
      BroadcasterCache cache;
      try {
        cache=(BroadcasterCache)Thread.currentThread().getContextClassLoader().loadClass(className).newInstance();
      }
 catch (      ClassNotFoundException ex) {
        cache=(BroadcasterCache)getClass().getClassLoader().loadClass(className).newInstance();
      }
      InjectorProvider.getInjector().inject(cache);
      setBroadcasterCache(cache);
    }
  }
 catch (  InstantiationException e) {
    throw new RuntimeException(e);
  }
catch (  IllegalAccessException e) {
    throw new RuntimeException(e);
  }
catch (  ClassNotFoundException e) {
    throw new RuntimeException(e);
  }
}
