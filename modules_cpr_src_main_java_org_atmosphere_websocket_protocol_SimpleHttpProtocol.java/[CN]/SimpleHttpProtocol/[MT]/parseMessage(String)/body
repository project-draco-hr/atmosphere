{
  String pathInfo=request().getPathInfo();
  if (d.startsWith(delimiter)) {
    String[] token=d.split(delimiter);
    pathInfo=token[1];
    d=token[2];
  }
  AtmosphereRequest r=new AtmosphereRequest.Builder().request(request()).method(methodType).contentType(contentType).body(d).pathInfo(pathInfo).headers(configureHeader(request())).build();
  dispatch(r,new WebSocketHttpServletResponse<WebSocket>(webSocketSupport()));
}
