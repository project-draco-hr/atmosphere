{
  try {
    String pathInfo=request().getPathInfo();
    if (d.startsWith(delimiter)) {
      String[] token=d.split(delimiter);
      pathInfo=token[1];
      d=token[2];
    }
    WebSocketHttpServletRequest r=new WebSocketHttpServletRequest.Builder().request(request()).method(methodType).contentType(contentType).body(d).pathInfo(pathInfo).headers(configureHeader(request())).build();
    atmosphereServlet().doCometSupport(r,new WebSocketHttpServletResponse<WebSocket>(webSocketSupport()));
  }
 catch (  IOException e) {
    logger.warn(e.getMessage(),e);
  }
catch (  ServletException e) {
    logger.warn(e.getMessage(),e);
  }
}
