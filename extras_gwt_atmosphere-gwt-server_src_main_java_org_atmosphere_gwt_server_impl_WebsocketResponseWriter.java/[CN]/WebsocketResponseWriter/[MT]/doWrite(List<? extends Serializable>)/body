{
  for (  Serializable message : messages) {
    CharSequence string;
    char event;
    if (message instanceof CharSequence) {
      string=(CharSequence)message;
      event='s';
    }
 else {
      string=serialize(message);
      event='o';
    }
    writer.append(event).append(";");
    writer.append(string).append(MESSAGE_SEPERATOR);
  }
}
