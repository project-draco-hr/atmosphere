{
  super.action(r);
  if (r.isResumed() && r.getRequest().getAttribute(ASYNCHRONOUS_HOOK) != null) {
    if (r.getRequest().getAttribute(CHANNEL) == null)     return;
    try {
      ((AsyncIOWriter)r.getRequest().getAttribute(CHANNEL)).close(r.getResponse(false));
    }
 catch (    IOException e) {
      logger.trace("",e);
    }
  }
}
