{
  AtmosphereFramework f=new AtmosphereFramework().addInitParameter("org.atmosphere.suppressDetectingJSR356","true");
  f.init();
  final AtomicBoolean b=new AtomicBoolean();
  f.getAtmosphereConfig().startupHook(new AtmosphereConfig.StartupHook(){
    @Override public void started(    AtmosphereFramework framework){
      b.set(true);
    }
  }
);
  assertTrue(b.get());
}
